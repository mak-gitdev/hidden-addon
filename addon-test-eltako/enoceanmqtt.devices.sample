#======================================================================
# For the moment, you must define two entries per FSB14
# The first one is for command and status based on A5-3F-7F telegram
[my_fsb14/a5]
address = 0x454C544B # The address of the FSB14. Must be the same address as below
rorg = 0xA5
func = 0x3F
type = 0x7F

# The second one is for status based on F6-02-01 telegram
[my_fsb14/f6]
address = 0x454C544B # The address of the FSB14. Must be the same address as above
rorg = 0xF6
func = 0x02
type = 0x01
